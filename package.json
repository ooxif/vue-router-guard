{
  "private": true,
  "name": "vue-router-guard",
  "version": "0.1.3",
  "description": "Extends route guards.",
  "author": "oo <oo@xif.at>",
  "license": "MIT",
  "main": "index.js",
  "jsnext:main": "index.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ooxif/vue-router-guard.git"
  },
  "scripts": {
    "build": "node scripts/build",
    "codeclimate": "cat ./coverage/lcov.info | codeclimate-test-reporter",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "standard",
    "test": "jest",
    "test:cover": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/__tests__/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  },
  "babel": {
    "presets": [
      "latest",
      "stage-0"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "peerDependencies": {
    "vue": "^2.3.2",
    "vue-router": "^2.5.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-preset-latest": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "codeclimate-test-reporter": "0.5.0",
    "codecov": "3.1.0",
    "coveralls": "2.13.1",
    "del": "3.0.0",
    "jest": "20.0.4",
    "rollup": "0.48.2",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-replace": "1.1.1",
    "standard": "10.0.3",
    "vue": "2.4.2",
    "vue-router": "2.7.0"
  }
}
